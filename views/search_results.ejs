<!DOCTYPE HTML>
<html>
  <head>
    <title>Results</title>
    <link rel="stylesheet" type="text/css" href="style.css" />
  </head>
  <body>
    <%- include('search-form.ejs') %>

    <p>You searched for: <%= query.slice(1, -1)%></p>
<% data.forEach(function (result) {%>
  <div id ="title"><%= result.title %></div>
  <div id ="weburl"><a href="<%= result.weburl %>"><%= result.weburl %></a></div>
  <%= result.description %><br>
  <%= result.keywords %><br>
<% }) %>


  </body>
</html>
